{
  "manifest_version": 2,
  "name": "Zotero S3 Sync",
  "version": "0.1",
  "description": "Zotero Plugin for S3 storage",
  "author": "Alain Borel",
  "homepage_url": "https://github.com/aborel",
  "icons": {
    "48": "chrome/skin/amazon-s3.svg",
    "96": "chrome/skin/amazon-s3.svg"
  },
  "applications": {
    "zotero": {
      "id": "zotero-s3@library.epfl.ch",
      "update_url": "https://raw.githubusercontent.com/aborel/zotero-s3/master/updates.json",
      "strict_min_version": "7.0",
      "strict_max_version": "7.999"
    }
  },
  "background": {
    "scripts": ["bootstrap.js"]
  }
}
